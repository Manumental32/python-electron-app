  "devDependencies": {
    "electron": "^18.3.7",
    "electron-packager": "^15.0.0"
  },
  "dependencies": {
    "python-shell": "^0.5.0"
  },
  "build": {
    "directories": {
      "buildResources": "resources",
      "output": "build"
    }
  }
